<sql-cases db-types="SQLServer" namespace="sqlserver.dcl.grant">
    <sql-case id="grant_select" value="GRANT SELECT ON database1 TO user1" />
    <sql-case id="grant_select_column" value="GRANT SELECT (order_id) ON database1 TO user1" />
